rooms:
  - name: Main
    oncreate: >-
      var placar = ct.rooms.list[1];

      ct.rooms.append('Placar');


      this.asteroidTimer = 20;

      this.enemyTimer = 180;

      this.itemTimer = 180;

      this.score = 0;

      this.lives = 3;


      this.scoreLabel = new PIXI.Text('Score: ' + this.score);

      this.scoreLabel = new PIXI.Text('Score: ' + this.score,
      ct.styles.get('ScoreText'));

      this.addChild(this.scoreLabel);

      this.scoreLabel.x = 580;

      this.scoreLabel.y = 30;

      this.scoreLabel.depth = 1000;


      this.livesLabel = new PIXI.Text('Lives: ' + this.lives,
      ct.styles.get('ScoreText'));

      this.addChild(this.livesLabel);

      this.livesLabel.x = 580;

      this.livesLabel.y = 60;

      this.livesLabel.depth = 1000;
    onstep: |-
      this.asteroidTimer -= ct.delta;
      if (this.asteroidTimer <= 0) {
          this.asteroidTimer = ct.random.range(20, 200);
          ct.types.copy(ct.random.dice('meteor_ao', 'meteor_ao2'), ct.random(540), -100);
      }

      this.enemyTimer -= ct.delta;
      if (this.enemyTimer <= 0) {
          this.enemyTimer = ct.random.range(180, 400);
          ct.types.copy('vacaborgue', ct.random(540), -100);
      }

      this.itemTimer -= ct.delta;
      if (this.itemTimer <= 0 && this.lives == 3) {
          this.itemTimer = ct.random.range(180, 400);
          ct.types.copy('campbells', ct.random(540), -100);
      }
    ondraw: |-
      this.scoreLabel.text = 'Score: ' + this.score;
      this.livesLabel.text = 'Lives: ' + this.lives;
