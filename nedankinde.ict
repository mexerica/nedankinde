rooms:
  - name: Main
    oncreate: |
      ct.rooms.append('Placar');

      this.xBoundE = 108;
      this.xBoundD = 540;

      this.asteroidTimer = 20;
      this.enemyTimer = 80;
      this.souTimer = 150;
      this.bocaTimer = 120;
      this.meriTimer = 150;
      this.mamaTimer = 150;
      this.eyeTimer = 150;
    onstep: |
      this.asteroidTimer -= ct.delta;
      if (this.asteroidTimer <= 0) {
          this.asteroidTimer = ct.random.range(20, 60);
          ct.types.copy(ct.random.dice('meteor_ao', 'meteor_ao2'), ct.random.range(-128, 640), -64)
      }

      this.enemyTimer -= ct.delta;
      if (ct.rooms.list['Placar']['0'].score > 500) {
          if (this.enemyTimer <= 0){
              this.enemyTimer = 80;
              ct.types.copy('vacaborgue', ct.random.range(-128, 300), -64);
              ct.types.copy('vacaborgue', ct.random.range(320, 640), -64);
              ct.types.copy('vacaborgue', -128, ct.random.range(0, 300));
              ct.types.copy('vacaborgue', 704, ct.random.range(0, 300));
          } 
      }    

      this.souTimer -= ct.delta;
      if (ct.rooms.list['Placar']['0'].score > 2000) {
          if (this.souTimer <= 0) {
              this.souTimer = ct.random.range(120, 220);
              ct.types.copy('naoSou', ct.random(this.xBoundD), -50);
          }
      }

      this.meriTimer -= ct.delta;
      if (ct.rooms.list['Placar']['0'].score > 1500) {
          if (this.meriTimer <= 0) {
              this.meriTimer = ct.random.range(120, 220);
              ct.types.copy('meriline', ct.random(this.xBoundD), -50);
          }
      }

      this.eyeTimer -= ct.delta;
      if (ct.rooms.list['Placar']['0'].score > 1500) {
          if (this.eyeTimer <= 0) {
              this.eyeTimer = ct.random.range(120, 220);
              ct.types.copy('oio', 320, 256);
          }
      }

      this.bocaTimer -= ct.delta;
      if (ct.rooms.list['Placar']['0'].score > 2500) {
          if (this.bocaTimer <= 0) {
              this.bocaTimer = ct.random.range(120, 220);
              ct.types.copy('boca', ct.random(this.xBoundD), -50);
          }
      }

      this.mamaTimer -= ct.delta;
      if (ct.rooms.list['Placar']['0'].score > 3000) {
          if (this.mamaTimer <= 0) {
              this.mamaTimer = ct.random.range(120, 220);
              ct.types.copy('mamae', 320, 128);
          }
      }
