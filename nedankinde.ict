skeletons: []
types:
  - name: tesouras
    depth: 0
    oncreate: ''
    onstep: |-
      /**
       * Move the ship
       * See Project > Actions and input methods panel
       * and "Actions" in the docs.
       */

      this.x += 8 * ct.delta * ct.actions.MoveX.value; // Move by X axis
      this.y += 8 * ct.delta * ct.actions.MoveY.value; 

      /**
       * Check whether the ship fell off the viewport
       */
      if (this.x < 108) { // Have the ship crossed the left border?
          this.x = 108; // Go back to the left border
      }
      if (this.x > 540) { // Have the ship crossed the right border?
          this.x = 540; // Go back to the right border
      }
      if (this.y < 0) { // Have the ship crossed the left border?
          this.y = 0; // Go back to the left border
      }
      if (this.y > 550) { // Have the ship crossed the right border?
          this.y = 550; // Go back to the right border
      }

      if (ct.actions.Shoot.pressed){
          ct.types.copy('laser_az', this.x, this.y);
      }

      this.move();

      var hostile = ct.place.occupied(this, this.x, this.y, 'Hostile');
      if (hostile) {
          hostile.kill = true;
          ct.room.lives --;
          if (ct.room.lives <= 0) {
              this.kill = true;
                  window.setTimeout(function() {
                  ct.rooms.switch('Main');
              }, 1000);
          }
      }

      var recover = ct.place.occupied(this, this.x, this.y, 'Recover');
      if (recover) {
          recover.kill = true;
          if (ct.room.lives < 3) {
              ct.room.lives ++;
          }
      }
    ondraw: ''
    ondestroy: ''
    texture: 9a9fbd9c-e346-4bd0-97ce-08c623b39e8e
    extends: {}
    uid: e3f48b26-f02c-4e86-be89-a76093cb1867
    lastmod: 1619903679140
  - name: naoSou
    depth: 0
    oncreate: |-
      this.speed = 3;
      this.direction = 270;
      this.bulletTimer = 60;
      this.ctype = 'Hostile';
    onstep: |-
      this.move();

      if (this.y > ct.viewHeight + 80) {
          this.kill = true;
      }

      var collided = ct.place.meet(this, this.x, this.y, 'laser_az');
      if (collided) {
          collided.kill = true;
          this.kill = true;
      }

      this.bulletTimer -= ct.delta;
      if (this.bulletTimer <= 0) {
          this.bulletTimer = 180;
          ct.types.copy('quinaife', this.x, this.y + 32);
      }
    ondraw: ''
    ondestroy: ''
    texture: 3ccbb6b0-1049-48a3-bb39-921a270d9217
    extends: {}
    uid: 802ab2b0-e829-4883-87d5-7834368c83c5
    lastmod: 1619902092786
