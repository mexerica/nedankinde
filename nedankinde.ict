  - name: meteor_ao
    depth: 0
    oncreate: |-
      this.speed = ct.random.range(1, 3);
      this.direction = ct.random.range(270 - 30, 270 + 30);
      this.depth = 1000;
      this.ctype = 'Hostile';
    onstep: |-
      this.move();

      if (this.y > ct.viewHeight + 80) {
          this.kill = true;
      }

      var collided = ct.place.meet(this, this.x, this.y, 'laser_az');
      if (collided) {
          collided.kill = true;
          this.kill = true;
          ct.room.score += 50;
      }
