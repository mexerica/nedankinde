types:
  - name: tesouras
    depth: 0
    oncreate: |
      this.scale.x = 0.7;
      this.scale.y = 0.7;

      this.timerTiro = new CtTimer(1000, 'veloTiro');
    onstep: |-
      /**
       * Move the ship
       * See Project > Actions and input methods panel
       * and "Actions" in the docs.
       */

      this.x += 8 * ct.delta * ct.actions.MoveX.value; // Move by X axis
      this.y += 8 * ct.delta * ct.actions.MoveY.value; 

      /**
       * Check whether the ship fell off the viewport
       */
      if (this.x < ct.room.xBoundE) { // Have the ship crossed the left border?
          this.x = ct.room.xBoundE; // Go back to the left border
      }
      if (this.x > ct.room.xBoundD) { // Have the ship crossed the right border?
          this.x = ct.room.xBoundD; // Go back to the right border
      }
      if (this.y < 0) { // Have the ship crossed the left border?
          this.y = 0; // Go back to the left border
      }
      if (this.y > 550) { // Have the ship crossed the right border?
          this.y = 550; // Go back to the right border
      }

      if (this.timerTiro.time >= 250)
          this.timerTiro.time = 0;

      if (ct.actions.Shoot.pressed){
          this.timerTiro.time = 0;
      }

      if (ct.actions.Shoot.down){
          if (this.timerTiro.time == 0) {
              ct.types.copy('laser_az', this.x, this.y);
          }
      }

      if (ct.actions.Shoot2.pressed){
          if (ct.rooms.list['Placar']['0'].special >  0){
              ct.types.copy('bumerangue', this.x, this.y);
              ct.rooms.list['Placar']['0'].special --;
          }
      }

      this.move();

      var hostile = ct.place.occupied(this, this.x, this.y, 'Hostile');
      if (hostile) {
          hostile.kill = true;
          //ct.room.placar.lives --;
          ct.rooms.list['Placar']['0'].lives --;
          if (ct.rooms.list['Placar']['0'].lives <= 0) {
              this.kill = true;
                  window.setTimeout(function() {
                  ct.rooms.switch('Main');
              }, 1000);
          }
      }

      var recover = ct.place.occupied(this, this.x, this.y, 'Recover');
      if (recover) {
          recover.kill = true;
          if (ct.rooms.list['Placar']['0'].lives < 5) {
              ct.rooms.list['Placar']['0'].lives ++;
          }
      }

      var boom = ct.place.occupied(this, this.x, this.y, 'Boom');
      if (boom) {
          boom.kill = true;
          if (ct.rooms.list['Placar']['0'].special < 5) {
              ct.rooms.list['Placar']['0'].special ++;
          }
      }
    ondraw: ''
    ondestroy: ''
